{
  "name": "entix-web-client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "NODE_ENV=development vite --host",
    "build": "NODE_ENV=production tsc && vite build && cp CNAME dist/",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "reinstall": "rm -rf node_modules && npm install",
    "update:entix-shared": "npm uninstall entix-shared && npm i entix-shared",
    "preview": "vite preview",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@tanstack/react-query": "^5.51.5",
    "@tanstack/react-query-devtools": "^5.51.5",
    "antd": "^5.17.4",
    "antd-img-crop": "^4.22.0",
    "antd-zod": "^5.1.0",
    "axios": "^1.7.2",
    "cloudinary": "^2.3.0",
    "entix-shared": "^0.0.37",
    "jotai": "^2.9.0",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hotkeys-hook": "^4.5.0",
    "react-intersection-observer": "^9.13.0",
    "react-router-dom": "^6.23.1",
    "timezones-list": "^3.0.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.6",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vite-tsconfig-paths": "^4.3.2"
  }
}
